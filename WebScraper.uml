        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_p525EABQEe-ziPgQOrU91w" name="loadProxyList" specification="_pU7AAABQEe-ziPgQOrU91w">
          <language>java</language>
          <body>try {
			File file = new File(PROXY_FILE);
			Scanner reader = new Scanner(file);

			while (reader.hasNextLine()) {
				String prx = reader.nextLine();
				if (prx.isBlank())
					continue;
				proxy.add(prx);
			}

			reader.close();
		} catch (Exception e) {
			debug(e.getMessage());
		}
