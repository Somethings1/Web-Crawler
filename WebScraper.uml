        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_p5NY0ABQEe-ziPgQOrU91w" name="filterBySimilarEntity" specification="_pvMhMABQEe-ziPgQOrU91w">
          <ownedParameter xmi:id="_p5NY0QBQEe-ziPgQOrU91w" name="source" type="_pOx7wABQEe-ziPgQOrU91w"/>
          <ownedParameter xmi:id="_p5NY0gBQEe-ziPgQOrU91w" type="_pP59IABQEe-ziPgQOrU91w" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>articleSet.removeIf(article -> {
			int count = 0;
			for (Entity entity: article.entities) {
				for (Entity toCompare: source.entities)
					if (entity.content.equals(toCompare.content))
						count++;
			}
			if (count &lt; 10) return false;
			return true;
		});
		return this;
	</body>
