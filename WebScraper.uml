        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_p4-vUABQEe-ziPgQOrU91w" name="getTotalArticle" specification="_pPJvMABQEe-ziPgQOrU91w">
          <ownedParameter xmi:id="_p4-vUQBQEe-ziPgQOrU91w" direction="return">
            <name xsi:nil="true"/>
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>int total = 0;
		File file = new File(COUNTER_FILE_NAME);
		Scanner reader = new Scanner(file);
		String s = reader.nextLine();
		total = Integer.parseInt(s);
		reader.close();
