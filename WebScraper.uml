        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_p5GEEABQEe-ziPgQOrU91w" name="applyGeneralFilter" specification="_pQLC4ABQEe-ziPgQOrU91w">
          <ownedParameter xmi:id="_p5GEEQBQEe-ziPgQOrU91w" name="query">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_p5GEEgBQEe-ziPgQOrU91w" value="*"/>
          </ownedParameter>
          <ownedParameter xmi:id="_p5GEEwBQEe-ziPgQOrU91w" type="_pP59IABQEe-ziPgQOrU91w" direction="return">
            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>articleSet.removeIf(article -> {
			for (String content: query) {
				for (String s: article.content) 
					if (contains(s, content)) 
						return false;
				
				if (contains(article.title, content)
				 || contains(article.summary, content))	
					return false;
				
				if (contains(article.webName, content))
