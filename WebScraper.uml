        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="__L6LAAkPEe-_9bSnsIpr9A" name="showRecommendation" specification="__E_qsAkPEe-_9bSnsIpr9A">
          <ownedParameter xmi:id="__L6LAQkPEe-_9bSnsIpr9A" name="query">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <language>java</language>
          <body>if (query.isBlank() || query.contains(&quot;&amp;&quot;) || query.contains(&quot;,&quot;) || query.contains(&quot;=&quot;)) {
			hideRecommendation();
			return;
		}
