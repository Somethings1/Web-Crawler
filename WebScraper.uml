            <name xsi:nil="true"/>
          </ownedParameter>
          <language>java</language>
          <body>super(scraperOptions, articleSet);
		loadProxyList();
		proxyCounter = 0;
		this.scraperOptions = scraperOptions;
		threadCounter++;

		for (String s : nextURL)
			this.nextURL.put(s, 0);
		for (String s : visitedURL)
			this.visitedURL.put(s, 0);
	</body>
